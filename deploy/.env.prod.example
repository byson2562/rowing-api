# Domain
DOMAIN=example.com

# Rails
# Generate with: docker compose -f docker-compose.prod.yml --env-file deploy/.env.prod run --rm backend bundle exec rails secret
SECRET_KEY_BASE=replace_me
# Set from backend/config/master.key if credentials are used.
RAILS_MASTER_KEY=replace_me

# MySQL
MYSQL_ROOT_PASSWORD=replace_me_strong_root_password
MYSQL_DATABASE=smart_rowing_production
MYSQL_USER=smart_rowing
MYSQL_PASSWORD=replace_me_strong_app_password

# Frontend
# Google Analytics 4 Measurement ID (e.g. G-XXXXXXXXXX)
NEXT_PUBLIC_GA_MEASUREMENT_ID=

# Optional: set a project name to avoid conflicts with local compose
COMPOSE_PROJECT_NAME=rowing_api_prod
